        "#Combine 3-minutes windows data into one DataFrame (only in case there are more than one df)\n",
        "def combine_with_loc(dfs_acce, dfs_loc):\n",
        "    df_new = dfs_acce.join(dfs_loc, lsuffix = '', rsuffix = '_loc', how = 'outer').interpolate()\n",
